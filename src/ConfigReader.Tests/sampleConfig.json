{
  "ReadOnly": false,
  "LinkFormat": "Tfs",
  "TocLevel": 3,
  "Exclude": [ "Dir1", "Dir2" ],
  "UrlsAsSnippets": [ "Url1", "Url2" ],
  "TocExcludes": [ "Exclude Heading1", "Exclude Heading2" ],
